// Place your key bindings in this file to overwrite the defaultsauto[]
[
  {
    "key": "ctrl+cmd+r",
    "command": "extension.runSpecLine"
  },
  {
    "key": "shift+cmd+t",
    "command": "-extension.runFileSpecs"
  },
  {
    "key": "cmd+l",
    "command": "-extension.runSpecLine"
  },
  {
    "key": "cmd+y",
    "command": "extension.runFileSpecs"
  },
  {
    "key": "cmd+y",
    "command": "-extension.runLastSpec"
  }
]
