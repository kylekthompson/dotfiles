// Place your key bindings in this file to overwrite the defaultsauto[]
[
  {
    "key": "shift+cmd+t",
    "command": "-extension.runFileSpecs"
  },
  {
    "key": "cmd+l",
    "command": "-extension.runSpecLine"
  },
  {
    "key": "cmd+y",
    "command": "-extension.runLastSpec"
  },
  {
    "key": "cmd+; c",
    "command": "-testing.runAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+r",
    "command": "test-explorer.run-test-at-cursor"
  },
  {
    "key": "cmd+r",
    "command": "test-explorer.run-file"
  }
]
